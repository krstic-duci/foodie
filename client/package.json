{
  "name": "client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "^2.1.3",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "tsc --noEmit",
    "prettier": "prettier --config .prettierrc --cache --check \"{src,server}/**/*.{ts,tsx}\" || { rm -f node_modules/.cache/prettier/.prettier-cache ; exit 1; }",
    "prettier:fix": "prettier --config .prettierrc --cache --write \"{src,server}/**/*.{ts,tsx}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint-config-prettier",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.31",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "eslint-config-prettier": "8.8.0",
    "prettier": "2.8.8",
    "typescript": "4.9.5"
  }
}
