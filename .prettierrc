{
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "none",
  "singleQuote": false,
  "semi": true,
  "jsxSingleQuote": true,
  "importOrder": ["@models", "@resolvers", "^[./]", "^[../]", "@utils","__generated__"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy",
    "classProperties"
  ]
}
